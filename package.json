    "nyc": "15.1.0"
  },
  "overrides": {
    "@vue/vue-loader-v15": "npm:vue-loader@^15.10.0"
  },
  "nyc": {
    "instrument": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "register": "babel-register",
    "include": [
      "apollo-server/**/*.js",
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "apollo-server/*.js",
      "src/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  }
